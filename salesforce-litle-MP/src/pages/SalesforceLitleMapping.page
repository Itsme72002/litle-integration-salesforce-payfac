                            <apex:actionSupport event="onchange" rerender="subMerchantEmailAddressDefaultTxtBox"
                                action="{!onSelectChangeSubMerchantEmailAddress}" />
                        </apex:selectList>
                        <apex:inputText value="{!currentMappings.subMerchantEmailAddressD__c}" id="subMerchantEmailAddressDefaultTxtBox"
                            disabled="{!currentMappings.subMerchantEmailAddressX__c}" />
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>

                <apex:pageBlockSectionItem >
                    <apex:pageBlockSectionItem >
                        <apex:outputText value="Phone" />
                    </apex:pageBlockSectionItem>
                    <apex:outputPanel >
                        <apex:selectList id="subMerchantPhoneList" value="{!currentMappings.subMerchantPhone__c}"
                            size="1">
                            <apex:selectOptions value="{!salesforceFields}" />
                            <apex:actionSupport event="onchange" rerender="subMerchantPhoneDefaultTxtBox"
                                action="{!onSelectChangeSubMerchantPhone}" />
                        </apex:selectList>
                        <apex:inputText value="{!currentMappings.subMerchantPhoneD__c}" id="subMerchantPhoneDefaultTxtBox"
                            disabled="{!currentMappings.subMerchantPhoneX__c}" />
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                <!-- Primary Contact end -->

            </apex:pageBlockSection>
			<apex:pageBlockButtons >
				<apex:commandButton id="saveMappingsBtn" value="Save" action="{!saveMappings}" />
		        <apex:commandButton id="cancelMappingsBtn" value="Cancel" action="{!cancelMappings}" />
	        </apex:pageBlockButtons>

        </apex:pageBlock>
        <!-- <apex:commandButton id="resetBtn" value="Reset All Mappings" action="{!resetMappings}"/> -->

    </apex:form>
</apex:page>