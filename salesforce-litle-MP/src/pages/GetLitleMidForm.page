                <apex:pageBlockSectionItem >
                    <apex:outputText value="Last Name" />
                    <apex:outputPanel styleClass="requiredInput" layout="block">
                        <apex:outputPanel styleClass="requiredBlock" layout="block" />
                        <apex:inputText value="{!subMerchantModel.subMerchantPrimaryContact.LastName}" id="subMerchantLastNameDefaultTxtBox" 
                        	title="{!helpTextConstant.primaryContactLastName}"/>
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                
                

                <apex:pageBlockSectionItem >
                    <apex:outputText value="Email Address" />
                    <apex:outputPanel styleClass="requiredInput" layout="block">
                        <apex:outputPanel styleClass="requiredBlock" layout="block" />
                        <apex:inputText value="{!subMerchantModel.subMerchantPrimaryContact.EmailAddress}"
                            id="subMerchantEmailAddressDefaultTxtBox" title="{!helpTextConstant.primaryContactEmailAddress}"/>
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                
               

                <apex:pageBlockSectionItem >
                    <apex:outputText value="Phone" />
                    <apex:outputPanel styleClass="requiredInput" layout="block">
                        <apex:outputPanel styleClass="requiredBlock" layout="block" />
                        <apex:inputText value="{!subMerchantModel.subMerchantPrimaryContact.Phone}" id="subMerchantPhoneDefaultTxtBox" 
                        	title="{!helpTextConstant.primaryContactPhone}"/>
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                
                
                <!-- Primary Contact end -->

                
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                        <apex:commandButton id="submitBtn" value="Submit" action="{!submit}" />
                        <apex:commandButton id="cancelBtn" value="Cancel" action="{!cancel}" />
                </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>