<div class="flush-all"><script>



(function(){
if ($('body').hasClass("rwd")){
var $top3across = $("#layout div.top-3-across");
var $bottom3across = $("#layout div.bottom-3-across");
var $top2across = $("#layout div.top-2-across");
var $bottom2across = $("#layout div.bottom-2-across");
var $mid2across = $("#layout div.mid-2-across");

var $fullwidthCurWidth = parseFloat($(".fullwidth").css('max-width'));


function equalizeHeights(forceAdjust){
	var needsAdjusting = false;
	//throttle the resize event, and tie the JS to the media queries
	//if ($(".fullwidth").width() != $fullwidthCurWidth || forceAdjust){
	if (parseFloat($(".fullwidth").css('max-width')) != $fullwidthCurWidth || forceAdjust){
		needsAdjusting = true;
	}	
	
	if (needsAdjusting){
		$fullwidthCurWidth = parseFloat($(".fullwidth").css('max-width'));
		if ($fullwidthCurWidth <= 480){
			$top3across.css("height","auto");
			$bottom3across.css("height","auto");
		}else if ($fullwidthCurWidth  > 480 && $fullwidthCurWidth < 990){
			$top2across.css("height","auto");
			$mid2across.css("height","auto");
			$bottom2across.css("height","auto");
			Util.equalHeights($top2across);
			Util.equalHeights($mid2across);
			Util.equalHeights($bottom2across);
		}else{
			$top3across.css("height","auto");
			$bottom3across.css("height","auto");
			Util.equalHeights($top3across);
			Util.equalHeights($bottom3across);
		}
	}
	
}//close equalizeHeights

equalizeHeights(true);
$(window).resize(function(){
equalizeHeights();
});
}//if body has class rwd
})();
</script></div>
							
							
								

							
							
