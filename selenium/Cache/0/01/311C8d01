<script>
(function(){
$('#flexsliderId0').flexslider({
	animation : "fade"
	,animationLoop : true
	,slideDirection : "horizontal"
	,slideshowSpeed : "5000" 
	,slideshow 	:  true
	,directionNav : true 
	,controlNav : true  
	
	
	
	
	
	
	,slideToStart: 0
	
	
	
	
	,before: function(slider) {
		slider.doMath();
	}
	,after: function(slider) {
		// updates for Omniture tracking
		var newPage = Page.getName() + (0 == 0 ? '' : ':carousel-1') + ':slide-' + (slider.currentSlide + 1);
		if ((newPage != s.pageName) && !slider.playing) {
			s.pageName = newPage;
			s.tl(true, 'o', newPage);
		}
	}
//	,controlsContainer: ".flex-container-0.flex-container"
	
	
	
});

$('.flex-direction-nav .prev, .flex-direction-nav .next').attr('href','');



})();

</script>








	

	
	
			
										
		

								
							

							
							
							
